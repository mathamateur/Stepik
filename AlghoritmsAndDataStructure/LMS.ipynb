   "source": [
    "n = int(input())\n",
    "a = list(map(int,input().split()))\n",
    "d = [0 for i in range(n)]\n",
    "for i in range(n):\n",
    "    d[i] = 1\n",
    "    for j in range(i):\n",
    "        if (a[i] % a[j] == 0) and (d[j] + 1 > d[i]):\n",
    "            d[i] = d[j] + 1\n",
    "ans = max(d)\n",
    "print(ans)\n",
    "            "
